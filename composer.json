{
    "name": "albaraa/aztec",
    "description": "Automatic CRUD generator for Laravel modules",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Albaraa\\Aztec\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Albaraa Alshalabi"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/filesystem": "^12.0",
        "nikic/php-parser": "^5.7",
        "doctrine/dbal": "^4.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Albaraa\\Aztec\\AztecServiceProvider"
            ]
        }
    }
}